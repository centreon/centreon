{
  "compilerOptions": {
    "downlevelIteration": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "es2018",
    "jsx": "react-jsx",
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}