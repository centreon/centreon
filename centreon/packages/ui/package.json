    "mochawesome": "^7.1.3",
    "msw": "2.7.3",
    "msw-storybook-addon": "^2.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-test-renderer": "^19.0.0",
    "remark-gfm": "^4.0.1",
    "speed-measure-vite-plugin": "^1.1.0",
    "storybook": "^8.6.14",
    "storybook-addon-mock": "^5.0.0",
    "storybook-dark-mode": "^4.0.2",
    "storybook-react-rsbuild": "^1.0.1",
    "style-dictionary": "^4.3.3",
    "tailwind": "^4.0.0",
    "tailwindcss": "^4.1.7",
    "ts-node": "^10.9.2",
    "use-resize-observer": "^9.1.0",
    "vite": "^6.3.5",
    "vite-plugin-istanbul": "^7.0.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-plugin-turbosnap": "^1.0.3"
  },
  "peerDependencies": {
    "@centreon/ui-context": "workspace:*"
  },
  "dependencies": {
    "@lexical/html": "^0.27.0",
    "@lexical/link": "^0.27.0",
    "@lexical/list": "^0.27.0",
    "@lexical/react": "^0.27.0",
    "@lexical/rich-text": "^0.27.0",
    "@lexical/selection": "^0.27.0",
    "@lexical/utils": "^0.27.0",
    "@mui/material": "^7.1.0",
    "@react-spring/web": "^9.7.5",
    "@visx/clip-path": "^3.12.0",
    "@visx/curve": "^3.12.0",
    "@visx/event": "^3.12.0",
    "@visx/group": "^3.12.0",
    "@visx/hierarchy": "^3.12.0",
    "@visx/legend": "^3.12.0",
    "@visx/pattern": "^3.12.0",
    "@visx/point": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "@visx/text": "^3.12.0",
    "@visx/threshold": "^3.12.0",
    "@visx/visx": "3.12.0",
    "@visx/zoom": "^3.12.0",
    "anylogger": "^1.0.11",
    "d3-array": "3.2.4",
    "dayjs": "^1.11.13",
    "highlight.js": "^11.11.1",
    "html-react-parser": "^5.2.2",
    "humanize-duration": "^3.32.1",
    "lexical": "^0.27.0",
    "notistack": "^3.0.2",
    "numeral": "^2.0.6",
    "ramda": "0.30.1",
    "react-grid-layout": "^1.5.0",
    "react-resizable": "^3.0.5",
    "react-router": "7",
    "react-transition-group": "^4.4.5",
    "sanitize-html": "^2.14.0",
    "ulog": "^2.0.0-beta.19"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./junit.xml"
  },
  "msw": {
    "workerDirectory": ["public"]
  },
  "nyc": {
    "report-dir": "coverage",
    "reporter": ["json"]
  }
}
