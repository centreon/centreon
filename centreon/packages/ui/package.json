{
  "name": "@centreon/ui",
  "version": "1.0.1",
  "description": "Centreon UI Components",
  "scripts": {
    "update:deps": "pnpx npm-check-updates -i --format group",
    "lint": "biome check ./src/ --error-on-warnings",
    "lint:fix": "pnpm lint --fix",
    "lint:ci": "biome ci ./src/ --error-on-warnings",
    "storybook": "storybook dev -p 9001 -c .storybook",
    "build:storybook": "storybook build -c .storybook -o .out",
    "test": "jest",
    "test:storybook:local": "test-storybook --url http://127.0.0.1:9001",
    "test:ci": "jest --silent --reporter=jest-junit",
    "cypress:ui": "cypress open --component --browser=chromium",
    "cypress:run:updateSnapshots": "cypress run --component --browser=chromium --env updateSnapshots=true",
    "cypress:run:coverage": "cypress run --component --browser=chromium --env codeCoverageTasksRegistered=true",
    "cypress:run": "cypress run --component --browser=chromium",
    "cypress:install": "cypress install",
    "cypress:cli": "./cypress/scripts/cypress-cli.sh",
    "tokens:transform": "TS_NODE_PROJECT=tsconfig.node.json ts-node style-dictionary.transform.ts",
    "install:arm:binaries": "pnpm i -D @swc/core-linux-arm64-gnu@1.6.6 @rspack/binding-linux-arm64-gnu@1.1.6 -w && pnpm remove @swc/core-linux-arm64-gnu @rspack/binding-linux-arm64-gnu"
  },
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/centreon/centreon.git"
  },
  "keywords": ["centreon", "react"],
  "author": {
    "name": "centreon@centreon.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/centreon/centreon/issues"
  },
  "homepage": "https://github.com/centreon/centreon#readme",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./components": "./src/components/index.ts",
    "./fonts/*": "./src/fonts/*",
    "./test/testRenderer": "./test/testRenderer.tsx",
    "./tailwindcss": "./src/ThemeProvider/tailwindcss.css"
  },
  "files": ["package.json", "src/*", "public/*", "test/*"],
  "devDependencies": {
    "@cypress/react": "^9.0.0",
    "@cypress/webpack-dev-server": "^4.0.1",
    "@faker-js/faker": "^9.5.1",
    "@mdx-js/react": "^3.1.0",
    "@rsbuild/core": "^1.3.21",
    "@rsbuild/plugin-react": "^1.3.1",
    "@simonsmith/cypress-image-snapshot": "^9.1.0",
    "@storybook/addon-a11y": "^8.6.3",
    "@storybook/addon-docs": "^8.6.3",
    "@storybook/addon-essentials": "^8.6.3",
    "@storybook/addon-interactions": "^8.6.3",
    "@storybook/addon-themes": "^8.6.3",
    "@storybook/blocks": "^8.6.3",
    "@storybook/manager-api": "^8.6.3",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/preview-api": "^8.6.3",
    "@storybook/react": "^8.6.3",
    "@storybook/react-vite": "^8.6.3",
    "@storybook/test": "^8.6.3",
    "@storybook/test-runner": "^0.22.0",
    "@storybook/theming": "^8.6.3",
    "@tailwindcss/cli": "^4.1.7",
    "@tailwindcss/postcss": "^4.1.7",
    "@tailwindcss/vite": "^4.1.7",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/ramda": "^0.30.2",
    "@types/react": "^19.0.10",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "chai": "^5.2.0",
    "cypress": "^14.1.0",
    "identity-obj-proxy": "^3.0.0",
