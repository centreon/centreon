{
  "name": "@centreon/ui",
  "version": "23.4.10-MON-build-npm-packages.10",
  "description": "Centreon UI Components",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "(cd ../ui-context && pnpm build:lib)",
    "eslint": "pnpm prebuild && eslint ./src --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "pnpm eslint --fix",
    "storybook": "pnpm prebuild && start-storybook -p 9001 -c .storybook",
    "build:storybook": "pnpm prebuild && build-storybook -c .storybook -o .out",
    "test": "pnpm prebuild && node testRunner.js",
    "test:storyshot": "pnpm prebuild && node testRunner.js storyshots.test.js",
    "test:ci": "pnpm prebuild && node testRunner.js --silent --reporter=jest-junit",
    "build:swc:only": "swc src --out-dir dist --sync --config-file ../js-config/swc/base.json",
    "build:lib": "pnpm build:swc:only & tsc",
    "build:lib:watch": "pnpm build:swc:only -w & tsc -w"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/centreon/centreon-frontend.git"
  },
  "keywords": [
    "centreon",
    "react"
  ],
  "author": "centreon@centreon.com",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/centreon/centreon-frontend/issues"
  },
  "homepage": "https://github.com/centreon/centreon-frontend#readme",
  "devDependencies": {
    "@mdx-js/react": "^2.1.4",
    "@storybook/addon-essentials": "^6.5.11",
    "@storybook/addon-storyshots": "^6.5.11",
    "@storybook/addon-storyshots-puppeteer": "^6.5.11",
    "@storybook/builder-webpack5": "^6.5.11",
    "@storybook/manager-webpack5": "^6.5.11",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/react": "^6.5.11",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "@types/jest": "^27.5.0",
    "@types/ramda": "^0.28.12",
    "axios-mock-adapter": "^1.20.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "puppeteer": "^13.7.0",
    "react-test-renderer": "^18.1.0",
    "storybook-addon-mock": "^2.4.1",
    "storybook-dark-mode": "^1.1.0"
  },
  "dependencies": {
    "anylogger": "^1.0.11",
    "humanize-duration": "^3.27.3",
    "notistack": "^2.0.8",
    "react-transition-group": "^4.4.5",
    "ulog": "^2.0.0-beta.19"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./junit.xml"
  }
}
