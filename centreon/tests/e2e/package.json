{
  "name": "centreon-cypress",
  "version": "21.04.0",
  "description": "centreon web package",
  "scripts": {
    "start:docker": "docker run -d -p 4000:80 --name centreon-dev registry-docker.centreon.com/docker-global/centreon-web-centos7:develop",
    "kill:docker": "docker stop centreon-dev && docker rm centreon-dev",
    "cypress:local": "pnpm start:docker && cypress run --e2e --config-file cypress.dev.config.ts --browser=chrome && pnpm kill:docker",
    "cypress:local:debug": "pnpm start:docker && DEBUG=cypress:* cypress run --e2e --config-file cypress.dev.config.ts --browser=chrome 2> ./cypress/results/logs.txt && pnpm kill:docker",
    "cypress:local:ui": "pnpm start:docker && cypress open --e2e --config-file cypress.dev.config.ts --browser=chrome && pnpm kill:docker",
    "eslint": "eslint ./cypress",
    "eslint:fix": "pnpm run eslint --fix"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^14.0.0",
    "@cypress/webpack-preprocessor": "^5.15.5",
    "@swc/core": "^1.2.203",
    "@types/cypress": "^1.1.3",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/node": "^14.14.44",
    "cypress": "^11.0.1",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-hooks": "^0.4.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "path": "^0.12.7",
    "shell-exec": "^1.1.2",
    "swc-loader": "^0.2.3",
    "typescript": "^4.7.3",
    "webpack": "^4.46.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
