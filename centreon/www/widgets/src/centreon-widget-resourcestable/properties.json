  "options": {
    "displayType": {
      "type": "displayType",
      "defaultValue": "all"
    },
    "statuses": {
      "type": "checkbox",
      "label": "Display resources with this status",
      "options": [
          {
            "id": "success",
            "name": "Success (OK & Up)"
          },
          {
            "id": "warning",
            "name": "Warning"
          },
          {
            "id": "problem",
            "name": "Problem (Down & Critical)"
          },
          {
            "id": "undefined",
            "name": "Undefined (Unreachable & Unknown)"
          },
          {
            "id": "pending",
            "name": "Pending"
          }
        ],
      "defaultValue": ["warning","problem"]
    },
    "states": {
      "type": "checkbox",
      "label": "Display resources with this state",
      "options": [
        {
          "id": "unhandled_problems",
          "name": "Unhandled"
        },
        {
          "id": "acknowledged",
          "name": "Acknowledged"
        },
        {
          "id": "in_downtime",
          "name": "In downtime"
        }
      ],
      "defaultValue": ["unhandled_problems"]
    },
    "refreshInterval": {
      "type": "refresh-interval",
      "defaultValue": "default",
      "label": "Interval"
    }
  },
  "data": {
    "resources": {
      "type": "resources",
      "defaultValue": [],
      "required" : false
    }
  },
  "moduleName": "centreon-widget-resourcestable"
}