        {
          "id": "0",
          "name": "This week"
        },
        {
          "id": "1",
          "name": "This month"
        },
        {
          "id": "2",
          "name": "This year"
        },
        {
          "id": "3",
          "name": "Last week"
        },
        {
          "id": "4",
          "name": "Last 6 months"
        },
        {
          "id": "5",
          "name": "Last 12 months"
        }
      ],
      "hiddenCondition": {
        "target": "options",
        "method": "includes",
        "when": "options.resourceTypes",
        "matches": ["host-group", "host-category"]
      }
    },
    "resourceTypes": {
      "type": "radio",
      "defaultValue": "host-group",
