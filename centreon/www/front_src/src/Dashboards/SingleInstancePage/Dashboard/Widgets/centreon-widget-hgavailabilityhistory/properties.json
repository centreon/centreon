        {
          "id": "host-group",
          "name": "Host groups"
        },
        {
          "id": "host-category",
          "name": "Host categories"
        },
        {
          "id": "host",
          "name": "Host"
        }
      ]
    },
    "threshold": {
      "type": "threshold",
      "defaultValue": {
        "enabled": true,
        "customCritical": null,
        "criticalType": "default",
        "customWarning": null,
        "warningType": "default"
      },
      "label": "threshold",
      "tooltipLabel": "Thresholds can be hidden if the custom min and max values of the Y axis are below/above the thresholds."
