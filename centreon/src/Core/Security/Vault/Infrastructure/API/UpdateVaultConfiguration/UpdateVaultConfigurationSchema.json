{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Update or create Vault Configuration",
    "type": "object",
    "required": [
        "address",
        "port",
        "root_path",
        "role_id",
        "secret_id"
    ],
    "additionalProperties": false,
    "properties": {
        "address": {
            "type": "string"
        },
        "port": {
            "type": "integer"
        },
        "root_path": {
            "type": "string"
        },
        "role_id": {
            "type": "string"
        },
        "secret_id": {
            "type": "string"
        }
    }
}
