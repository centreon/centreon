    "require-dev": {
        "behat/behat": "^3.10",
        "behat/mink-selenium2-driver": "^1.5",
        "friends-of-behat/mink": "^1.9",
        "friends-of-behat/mink-extension": "^2.5",
        "centreon/centreon-test-lib": "22.10.x-dev",
        "squizlabs/php_codesniffer": "3.5.8",
        "psr/container": "1.1.1"
    }
}
