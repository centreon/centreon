{
  "extends": "../../../centreon/packages/js-config/tsconfig/index",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "target": "ES2020",
    "module": "commonjs",
    "types": ["cypress", "cypress-wait-until", "node"],
    "paths": {
      "*": ["node_modules/*"],
      "@badeball/cypress-cucumber-preprocessor/*": ["./node_modules/@badeball/cypress-cucumber-preprocessor/dist/subpath-entrypoints/*"],
      "@bahmutov/cypress-esbuild-preprocessor": ["./node_modules/@bahmutov/cypress-esbuild-preprocessor"]
    },
    "skipLibCheck": true,
    "strict": true
  },
  "include": [
    "**/*.ts",
    "**/*.js",
    "support/**/*.ts",
    "plugins/**/*.ts"
  ]
}
