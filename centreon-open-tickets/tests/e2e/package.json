{
  "name": "centreon-open-tickets-cypress",
  "version": "23.10.0",
  "description": "Cypress end-to-end tests for open tickets",
  "scripts": {
    "cypress:run": "cross-env TESTCONTAINERS_RYUK_DISABLED=true cypress run --env tags='not @ignore' --config-file cypress.config.ts --browser=chrome --e2e",
    "cypress:open": "cross-env TESTCONTAINERS_RYUK_DISABLED=true cypress open --config-file cypress.dev.config.ts --browser=chrome --e2e",
    "cypress:ci": "cross-env TESTCONTAINERS_RYUK_DISABLED=true cypress run --e2e --browser=chrome",
    "cypress:install": "cypress install",
    "eslint": "eslint ./cypress",
    "eslint:fix": "pnpm eslint --fix"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@centreon/js-config": "24.11.0",
    "@cypress/webpack-preprocessor": "^6.0.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@swc/core": "^1.3.104",
    "@types/cypress-cucumber-preprocessor": "^4.0.5",
    "@types/dockerode": "^3.3.23",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "cross-env": "^7.0.3",
    "cypress": "~13.6.3",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-on-fix": "^1.0.3",
    "cypress-real-events": "^1.12.0",
    "cypress-terminal-report": "^5.3.10",
    "cypress-wait-until": "^3.0.1",
    "dockerode": "^4.0.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-hooks": "^0.4.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "mochawesome": "^7.1.3",
    "mysql2": "^3.9.1",
    "path": "^0.12.7",
    "prettier": "^3.0.0",
    "shell-exec": "^1.1.2",
    "swc-loader": "^0.2.3",
    "tar-fs": "^3.0.5",
    "testcontainers": "^10.7.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.2"
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": false,
    "nonGlobalStepDefinitions": true,
    "omitFiltered": true,
    "json": {
      "enabled": true,
      "output": "results/cucumber-logs/report.json"
    },
    "pretty": {
      "enabled": true
    }
  }
}
