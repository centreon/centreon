version: '2'
services:
  web:
    image: @WEB_IMAGE@
    ports:
    - "80"
    - "3306"
    links:
    - openldap
  openldap:
    image: registry.centreon.com/mon-openldap:latest
    ports:
    - "389"
  webdriver:
    image: registry.centreon.com/standalone-chrome-debug:3.141.59-oxygen
    environment:
    - "SCREEN_WIDTH=1600"
    - "SCREEN_HEIGHT=2000"
    ports:
    - "4444"
    - "5900"
    volumes:
    - /dev/shm:/dev/shm
