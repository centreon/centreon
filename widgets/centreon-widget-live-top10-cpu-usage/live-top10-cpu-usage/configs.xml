<configs>
        <title>Live Top 10 CPU Usage</title>
        <author>Centreon</author>
        <email>contact@centreon.com</email>
        <website>http://www.centreon.com</website>
        <description>Widget for top 10 hosts in function % cpu</description>
        <version>1.0.0</version>
        <keywords>centreon, widget, cpu</keywords>
        <screenshot></screenshot>
        <thumbnail>./widgets/live-top10-cpu-usage/resources/logo1.png</thumbnail>
        <url>./widgets/live-top10-cpu-usage/index.php</url>
        <autoRefresh>10</autoRefresh>
	<preferences>
	  <preference label="Service Description" name="service_description" defaultValue="cpu" type="text" header="Filter"/>
	  <preference label="Metric Name" name="metric_name" defaultValue="cpu" type="text"/>
	  <preference label="Nombre de ligne" name="nb_lin" defaultValue="10" type="text"/>
	  <preference label="Name Host Group" name="host_group" defaultValue="" type="hostgroup" header="Resource"/>
	 </preferences>
</configs>
